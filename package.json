{
  "name": "ng2-toasty",
  "description": "Angular2 Toasty component shows growl-style alerts and messages for your web app",
  "version": "0.0.0-semantically-released",
  "scripts": {
    "test": "ngc && karma start",
    "test-watch": "ngc && karma start --no-single-run --auto-watch",
    "minify": "node node_modules/uglify-js/bin/uglifyjs bundles/ng2-toasty.js -o bundles/ng2-toasty.min.js --source-map bundles/ng2-toasty.min.js.map -c -m",
    "prepublish": "typings install && ngc && node make.js && npm run minify",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akserg/ng2-toasty.git"
  },
  "keywords": [
    "angular",
    "angular2",
    "toasty",
    "growl"
  ],
  "author": "Sergey Akopkokhyants",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akserg/ng2-toasty/issues"
  },
  "main": "index.js",
  "typings": "index",
  "homepage": "https://github.com/akserg/ng2-toasty",
  "dependencies": {
    "@angular/compiler-cli": "^2.2.2",
    "@angular/platform-server": "^2.2.2",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "@angular/common": "^2.2.2",
    "@angular/compiler": "^2.2.2",
    "@angular/core": "^2.2.2",
    "@angular/platform-browser": "^2.2.2",
    "@angular/platform-browser-dynamic": "^2.2.2",
    "commitizen": "^2.7.6",
    "cz-conventional-changelog": "^1.1.5",
    "jasmine-core": "~2.5.1",
    "karma": "~1.3.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-firefox-launcher": "~1.0.0",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "~1.0.2",
    "semantic-release": "^4.3.5",
    "systemjs": "~0.19.27",
    "systemjs-builder": "^0.15.6",
    "typescript": "2.0.6",
    "typings": "^1.3.3",
    "uglify-js": "^2.6.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
